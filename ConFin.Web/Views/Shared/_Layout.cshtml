<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>ConFin - Controle Financeiro Pessonal</title>
    <!--Import Google Icon Font-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!--Import materialize.css-->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.99.0/css/materialize.min.css">

    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!--Import jQuery before materialize.js-->
    <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.99.0/js/materialize.min.js"></script>

    <script>
        $(document).ready(function () {$('select').material_select();});
    </script>

</head>
<body>

    <ul id="dropdown1" class="dropdown-content">
        <li><a href="#!">Perfil</a></li>
        <li><a href="#!">Configuração</a></li>
        <li class="divider"></li>
        <li><a href="#!">Sair</a></li>
    </ul>
    <nav>
        <div class="nav-wrapper">
            <a href="#!" class="brand-logo"><i class="material-icons">cloud</i>ConFin - Controle Financeiro Pessoal</a>
            <ul class="right hide-on-med-and-down">
                <li><a href="sass.html">Contas</a></li>
                <li><a href="badges.html">Lançamentos</a></li>
                <!-- Dropdown Trigger -->
                <li><a class="dropdown-button" href="#!" data-activates="dropdown1">@ViewBag.Email<i class="material-icons right">arrow_drop_down</i></a></li>
            </ul>
        </div>
    </nav>

    @RenderBody()

</body>
</html>